# Venice API Configuration
VENICE_API_KEY=your_venice_api_key_here
VENICE_BASE_URL=https://api.venice.ai/api/v1

# MongoDB Configuration
# Format: mongodb+srv://username:password@cluster.mongodb.net/database?retryWrites=true&w=majority
DATABASE_URL=mongodb+srv://username:password@cluster.mongodb.net/agentic_app?retryWrites=true&w=majority

# Server Configuration
PORT=3001
NODE_ENV=development

# CORS Configuration
CORS_ORIGIN=http://localhost:5173

# JWT Configuration (for authentication if needed)
JWT_SECRET=your_jwt_secret_key_here_generate_a_random_string

# Logging Configuration
LOG_LEVEL=info

# Railway-specific Environment Variables (for production)
# These will be set automatically in Railway dashboard
# RAILWAY_ENVIRONMENT=production
# RAILWAY_PROJECT_NAME=your-project-name
# RAILWAY_SERVICE_NAME=api

# Frontend URL (for CORS in production)
FRONTEND_URL=https://your-frontend-domain.railway.app